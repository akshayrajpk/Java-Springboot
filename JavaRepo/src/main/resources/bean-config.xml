<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="GFG" class="com.example.javabasic.day11_12.springCore.program7.GFG">
        <property name="geek">
            <ref bean="CsvGFG" />
        </property>
    </bean>

    <bean id="CsvGFG" class="com.example.javabasic.day11_12.springCore.program7.CsvGFG" />
    <bean id="JsonGFG" class="com.example.javabasic.day11_12.springCore.program7.JsonGFG" />

</beans>
